
@{
    ViewBag.Title = "Home Page";
}

@{Html.RenderAction("StockPrices", "Home");}

@section scripts
{
    <script>
        var url = '@Url.Action("StockPrices", "Home")';
        var notifications = $("#stock-prices-table");
        setInterval(function () {
            notifications.load(url);
        }, 3000);
    </script>
}